            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "AdminApplication",
            "request": "launch",
            "mainClass": "xiaozhi.AdminApplication",
            "projectName": "xiaozhi-esp32-api"
        },
        {
            "type": "java",
            "name": "PasswordUtils",
            "request": "launch",
            "mainClass": "xiaozhi.modules.security.password.PasswordUtils",
            "projectName": "xiaozhi-esp32-api"
        }
    ]
